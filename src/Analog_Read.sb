#include "pin_defs.sblib"

DIM rc
rc = GpioSetFunc(TEMP_DATA, 3, 0x11)
PRINT "\n GPIO set code: ";rc

DIM read
read = GpioRead(TEMP_DATA)
PRINT "\n Value read: ";read

DIM temp

temp = ((read*115) / 1000) - 50
PRINT "\nCurrent Temp: "; temp
PRINT " Celsius"


