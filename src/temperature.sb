#include "lib/pin_defs.sblib"
#include "lib/gpio.sblib"

DIM rc
rc = GpioSetFunc(TEMP_DATA, ANALOG_IN, ADC_10BIT)
PRINT "\n GPIO set code: ";rc

DIM read
read = GpioRead(TEMP_DATA)
PRINT "\n Value read: ";read

DIM temp

temp = ((read*115) / 1000) - 50
PRINT "\nCurrent Temp: "; temp
PRINT " Celsius"